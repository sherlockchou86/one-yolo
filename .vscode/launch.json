{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug one-yolo samples",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/samples/${fileBasenameNoExtension}",
            "args": [
                ""
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/..",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build one-yolo project"
        }
    ]
}